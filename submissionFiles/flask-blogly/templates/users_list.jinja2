{% extends 'base.jinja2' %} {% block content %}
<h1 class = "p-2">Full list of Users</h1>
<ul>
  {% for user in users%}
  <a href = "/users/{{user.id}}"><li>{{user.toString()}}</li></a>
  {% endfor%}
</ul>
<form action="/users/new">
  <button type="submit">Add new user</button>
</form>
{% endblock %}
