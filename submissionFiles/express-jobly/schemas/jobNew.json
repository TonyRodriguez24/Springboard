{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": ["title", "salary", "companyHandle"],
  "properties": {
    "title": {
      "type": "string",
      "default": "",
      "title": "The title Schema",
      "examples": ["Accommodation manager"]
    },
    "salary": {
      "type": "integer",
      "default": 0,
      "title": "The salary Schema",
      "examples": [126000]
    },
    "equity": {
      "type": ["number", "null"],
      "minimum": 0,
      "maximum": 1,
      "title": "The equity Schema",
      "examples": [0.1, 0.5, null]
    },

    "companyHandle": {
      "type": "string",
      "default": "",
      "title": "The companyHandle Schema",
      "examples": ["mejia-scott-ryan"]
    }
  },
  "examples": [
    {
      "title": "Accommodation manager",
      "salary": 126000,
      "equity": null,
      "companyHandle": "mejia-scott-ryan"
    }
  ]
}
